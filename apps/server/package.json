{
   "name": "server",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "dev": "cross-env NODE_ENV=dev nest start --watch",
      "build": "nest build",
      "debug": "cross-env NODE_ENV=dev nodemon --watch 'src/**/*.ts' --exec 'node --inspect -r ts-node/register -r tsconfig-paths/register' src/main.ts",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
      "start:prod": "cross-env NODE_ENV=prod node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
      "generate": "cross-env NODE_ENV=dev drizzle-kit generate --out src/drizzle/migrations --schema src/drizzle/schema/schema.ts --dialect postgresql"
   },
   "dependencies": {
      "@nestjs/common": "^10.0.0",
      "@nestjs/config": "^3.3.0",
      "@nestjs/core": "^10.0.0",
      "@nestjs/mapped-types": "*",
      "@nestjs/platform-express": "^10.0.0",
      "dotenv": "^16.4.7",
      "drizzle-orm": "^0.33.0",
      "effect": "^3.11.9",
      "postgres": "^3.4.5",
      "reflect-metadata": "^0.2.0",
      "rxjs": "^7.8.1",
      "@repo/common": "*"
   },
   "devDependencies": {
      "@nestjs/cli": "^10.0.0",
      "@nestjs/schematics": "^10.0.0",
      "@nestjs/testing": "^10.0.0",
      "@repo/eslint-config": "*",
      "@repo/typescript-config": "*",
      "@repo/jest-config": "*",
      "@types/express": "^5.0.0",
      "@types/node": "^20.3.1",
      "@types/supertest": "^6.0.0",
      "@typescript-eslint/eslint-plugin": "^8.0.0",
      "@typescript-eslint/parser": "^8.0.0",
      "drizzle-kit": "^0.24.2",
      "source-map-support": "^0.5.21",
      "supertest": "^7.0.0",
      "ts-loader": "^9.4.3",
      "ts-node": "^10.9.1",
      "tsconfig-paths": "^4.2.0"
   }
}
